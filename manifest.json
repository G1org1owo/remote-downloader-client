{
  "name": "Remote Downloader",
  "version": "1.2",
  "manifest_version": 3,
  "minimum_chrome_version": "116",
  "permissions": ["downloads", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "settings.html",
  "action": {
    "default_popup": "popup.html"
  }
}